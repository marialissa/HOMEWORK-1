    #ITEM-02

        #grupo-italiano

        renda_italiana <- c(2.3,2.1,1.2,2.8,3.4,1.1,1.7,3.2)
        exp_italiana <- c(2,15,13,20,32,29,7,23)
        cat("renda média italiana =", mean(renda_italiana), "\n")
        cat("experiencia média italiana =", mean(exp_italiana), "\n")

        #grupo-ingles

        renda_inglesa <- c(1.6,2.7)
        exp_inglesa <- c(8,23)
        cat("renda média inglesa =", mean(renda_inglesa), "\n")
        cat("experiencia média inglesa =", mean(exp_inglesa), "\n")

        #grupo-belga

        renda_belga <- c(1.2,1.4)
        exp_belga <- c(21,5)
        cat("renda média belga =", mean(renda_belga), "\n")
        cat("experiencia média belga =", mean(exp_belga), "\n")

        #grupo-espanhol

        renda_espanhola <- c(0.9,1.6,1.2)
        exp_espanhola <- c(1,3,0)
        cat("renda média espanhola =", mean(renda_espanhola), "\n")
        cat("experiencia média espanhola =", mean(exp_espanhola), "\n")

        #grupo-frances

        renda_francesa <- c(1.8,1.6,2.0)
        exp_francesa <- c(28,1,19)
        cat("renda média francesa =", mean(renda_francesa), "\n")
         cat("experiencia média francesa =", mean(exp_francesa), "\n")

